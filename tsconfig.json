{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [
        "es6"
    ],
    "typeRoots": ["./node_modules/@types"],
    "allowJs": true,
    "outDir": "build",
    "rootDir": "src",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
}
}

